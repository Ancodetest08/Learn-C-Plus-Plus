#include <iostream>
#include <vector>

int main() {

	int numbers_one[5] = {1, 2, 3, 4, 5};
	int numbers_two[2][3] = {
		{1, 2, 3}
		{4, 5, 6}
	}; 

	std::cout << numbers_one[1] << std::endl;						// 2
	std::cout << numbers_two[1][2] << std::endl;					// 6
	
	vector<int> numbers;
	numbers.push_back(1);											// [1]
	numbers.push_back(8);											// [1, 8]
	numbers.size();													// 2
	numbers.erase(numbers.begin() + 1);								// [1]
	
	return 0;

}
