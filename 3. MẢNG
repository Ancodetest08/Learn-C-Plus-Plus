#include <iostream>

int main() {

	int numbers_one[5] = {1, 2, 3, 4, 5};
	int numbers_two[10][10] = {
		{1, 2, 3}
		{4, 5, 6}
	}; 

	std::cout << numbers_one[1] << std::endl;
	std::cout << numbers_two[2][2] << std::endl;

	// Thuật toán sắp xếp bubble sort
	for (int i = n - 1; i > 0; i--) {
		for (int j = 0; j < i; j++) {
			if (arr[j] > arr[j+1]) {
				int tmp = arr[j];
				arr[j] = arr[j+1];
				arr[j+1] = tmp;
			}
		}
	}

	return 0;

}
