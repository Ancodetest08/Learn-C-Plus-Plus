#include <iostream>
#include <string>

void print_message(string message) {
	std::cout << "Message: " << message << std::endl;
}

float avg(int arr[], int n) {
	int sum = 0;
	for (int i = 0; i < n; i++) {
		sum += arr[i];
	}
	return (float)sum / n;
}

int main() {

	int arr[10] = {1, 2, 3, 4};

	print_message("Hello Cac Ban.");
	std::cout << avg(arr, 4) <<std::endl;

	return 0;

}
