#include <iostream>
#include <string>

int main() {

	int n;
	string strg;
	string str = "Hello World";
	string str1 = "C++";
	string str2 = "H***World";
	string str3 = "World";

	std::cin >> n;
	cin.ignore();
	getline(cin, strg);

	str.substr(2, 5);					# llo W
	str.substr(3);						# lo World
	str.find("llo");					# 2
	str.erase(2, 3);					# str = "He World"
	str.replace(1, 2, "***");			# str = "H***World"
	str.compare(str1);					# -1
	str.compare(str2);					# 0
	str.compare(str3);					# 1
	str.insert(1, "**");				# str = "H*****World"
	str.length();						# 11
	str.size();							# 11
	tolower(str);						# str = "h*****world"
	
	return 0;

}
